1:\n","                tiempo_descarga /= 2  # Si solo hay un barco, se reduce a la mitad el tiempo de descarga\n","                print(f\"Solo hay un barco, reduciendo tiempo de descarga a {tiempo_descarga:.2f} días.\")\n","\n","            yield self.env.timeout(tiempo_descarga)\n","\n","\n","            # Es el segundo barco en el puerto\n","            if len(self.gruas.users) == 2:\n","                # Duplicar el tiempo de descarga restante del primer barco\n","                for req in self.gruas.queue:\n","                    tiempo_restante = req.triggered_at - self.env.now\n","                    if tiempo_restante > 0:\n","                        tiempo_descarga = tiempo_restante*2\n","                        print(f\"Segundo barco llegó, duplicando tiempo de descarga restante a {tiempo_descarga:.2f} días.\")\n","\n","            tiempos_en_puerto.append(tiempo_descarga)  # Registro del tiempo en el puerto\n","            print(f\"Barco {barco} descargado en el puerto en {tiempo_descarga:.2f} días.\")\n","\n","            # Registro de la ocupación del amarre\n","            for amarre, ocupacion in tiempos_ocupacion_amarre.items():\n","                if not self.amarres.queue:\n","                    tiempos_ocupacion_amarre[amarre] += tiempo_descarga\n","\n","def llegada_barco(env, puerto):\n","    while True:\n","        yield env.timeout(random.expovariate(1 / media_llegadas)) #Tiempo de llegada\n","        env.process(atender_barco(env, puerto, len(puerto.amarres.queue)))\n","\n","def atender_barco(env, puerto, num_esperando):\n","    print(f\"Llega un barco al puerto en el día {env.now}. Hay {num_esperando} en espera.\")\n","    with puerto.amarres.request() as req:\n","        yield req #Este comando hace que la ejecución de la función se detenga hasta que se satisfaga la solicitud de amarre.\n","        print(f\"Barco atracado en el día {env.now}.\")\n","        yield env.process(puerto.descarga_barco(num_esperando))\n","\n","# Función de la simulación\n","def simulacion_puerto(env, num_amarres, num_gruas):\n","    puerto = Puerto(env, num_amarres, num_gruas)\n","    env.process(llegada_barco(env, puerto))\n","    yield env.timeout(duracion_simulacion)\n","\n","# Ejecutar la simulación\n","env = simpy.Environment()\n","env.process(simulacion_puerto(env, num_amarres=2, num_gruas=2))\n","env.run(until=duracion_simulacion)\n","\n","# Calcular estadísticas\n","tiempo_minimo = min(tiempos_en_puerto)\n","tiempo_maximo = max(tiempos_en_puerto)\n","tiempo_promedio = sum(tiempos_en_puerto) / len(tiempos_en_puerto)\n","utilizacion_gruas = sum(tiempos_descarga) / (2 * duracion_simulacion)  # 2 porque hay 2 grúas\n","utilizacion_amarres = {amarre: ocupacion / duracion_simulacion for amarre, ocupacion in tiempos_ocupacion_amarre.items()}\n","\n","# Imprimir estadísticas\n","print(\"\\nEstadísticas:\")\n","print(f\"Tiempo mínimo en puerto: {tiempo_minimo:.2f} días\")\n","print(f\"Tiempo máximo en puerto: {tiempo_maximo:.2f} días\")\n","print(f\"Tiempo promedio en puerto: {tiempo_promedio:.2f} días\")\n","print(f\"Utilización esperada de grúas: {utilizacion_gruas:.2f}\")\n","for amarre, utilizacion in utilizacion_amarres.items():\n","    print(f\"Utilización esperada de {amarre}: {utilizacion:.2f}\")\n","\n","# Graficar histograma\n","plt.figure(figsize=(10, 6))\n","plt.hist(tiempos_en_puerto, bins=20, alpha=0.7, color='blue', edgecolor='black')\n","plt.axvline(x=np.mean(tiempos_en_puerto), color='red', linestyle='--', label='Promedio')\n","plt.axvline(x=np.mean(tiempos_en_puerto) + np.std(tiempos_en_puerto), color='green', linestyle='--', label='Desviación estándar')\n","plt.axvline(x=np.mean(tiempos_en_puerto) - np.std(tiempos_en_puerto), color='green', linestyle='--')\n","plt.xlabel('Tiempo en puerto (días)')\n","plt.ylabel('Frecuencia')\n","plt.title('Histograma de tiempos en puerto')\n","plt.legend()\n","plt.grid(True)\n","plt.show()\n"]}]}